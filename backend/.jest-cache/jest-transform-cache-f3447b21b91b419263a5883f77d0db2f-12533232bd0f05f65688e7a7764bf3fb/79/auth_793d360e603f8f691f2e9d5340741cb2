/* istanbul ignore next */
cov_wmf6zey3v().s[251]++;
router.post('/reset-password', [body('token').notEmpty().trim(), body('newPassword').isLength({
  min: 8,
  max: 128
}), body('confirmPassword').notEmpty().trim()], handleValidationErrors, async (req, res) => {
  /* istanbul ignore next */
  cov_wmf6zey3v().f[12]++;
  cov_wmf6zey3v().s[252]++;
  try {
    const {
      token,
      newPassword,
      confirmPassword
    } =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[253]++, req.body);

    // Check if new passwords match
    /* istanbul ignore next */
    cov_wmf6zey3v().s[254]++;
    if (newPassword !== confirmPassword) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[75][0]++;
      cov_wmf6zey3v().s[255]++;
      return res.status(400).json({
        error: 'Passwords do not match',
        message: 'New password and confirmation password must be same',
        messageBn: '     '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[75][1]++;
    }

    // Find reset token
    const resetToken =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[256]++, await prisma.emailVerificationToken.findUnique({
      where: {
        token
      },
      include: {
        user: true
      }
    }));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[257]++;
    if (!resetToken) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[76][0]++;
      cov_wmf6zey3v().s[258]++;
      return res.status(400).json({
        error: 'Invalid token',
        message: 'Password reset token is invalid or has been used',
        messageBn: '      '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[76][1]++;
    }

    // Check if token has expired
    cov_wmf6zey3v().s[259]++;
    if (resetToken.expiresAt < new Date()) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[77][0]++;
      cov_wmf6zey3v().s[260]++;
      // Delete expired token
      await prisma.emailVerificationToken.delete({
        where: {
          id: resetToken.id
        }
      });
      /* istanbul ignore next */
      cov_wmf6zey3v().s[261]++;
      return res.status(400).json({
        error: 'Token expired',
        message: 'Password reset token has expired. Please request a new one.',
        messageBn: '        '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[77][1]++;
    }

    // Validate new password strength
    const userInfo =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[262]++, {
      firstName: resetToken.user.firstName,
      lastName: resetToken.user.lastName,
      email: resetToken.user.email,
      phone: resetToken.user.phone
    });
    const passwordValidation =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[263]++, passwordService.validatePasswordStrength(newPassword, userInfo));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[264]++;
    if (!passwordValidation.isValid) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[78][0]++;
      cov_wmf6zey3v().s[265]++;
      return res.status(400).json({
        error: 'Password does not meet requirements',
        message: 'New password is too weak or does not meet security requirements',
        messageBn: '     ',
        details: {
          strength: passwordValidation.strength,
          score: passwordValidation.score,
          feedback: passwordValidation.feedback,
          warnings: passwordValidation.warnings,
          suggestions: passwordValidation.suggestions,
          passwordPolicy: passwordService.getPasswordPolicy()
        }
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[78][1]++;
    }

    // Check if password has been used before
    const isPasswordReused =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[266]++, await passwordService.isPasswordReused(resetToken.userId, newPassword));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[267]++;
    if (isPasswordReused) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[79][0]++;
      cov_wmf6zey3v().s[268]++;
      return res.status(400).json({
        error: 'Password already used',
        message: 'You cannot reuse a password that has been used in the last 5 changes',
        messageBn: '  5        '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[79][1]++;
    }

    // Hash new password
    const hashedNewPassword =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[269]++, await passwordService.hashPassword(newPassword));

    // Update user password
    /* istanbul ignore next */
    cov_wmf6zey3v().s[270]++;
    await prisma.user.update({
      where: {
        id: resetToken.userId
      },
      data: {
        password: hashedNewPassword
      }
    });

    // Save new password to history
    /* istanbul ignore next */
    cov_wmf6zey3v().s[271]++;
    await passwordService.savePasswordToHistory(resetToken.userId, hashedNewPassword);

    // Delete reset token
    /* istanbul ignore next */
    cov_wmf6zey3v().s[272]++;
    await prisma.emailVerificationToken.delete({
      where: {
        id: resetToken.id
      }
    });

    // Update user status to active if pending
    /* istanbul ignore next */
    cov_wmf6zey3v().s[273]++;
    if (resetToken.user.status === 'PENDING') {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[80][0]++;
      cov_wmf6zey3v().s[274]++;
      await prisma.user.update({
        where: {
          id: resetToken.userId
        },
        data: {
          status: 'ACTIVE'
        }
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[80][1]++;
    }

    // Log password reset
    cov_wmf6zey3v().s[275]++;
    console.log(`Password reset for user ${resetToken.userId} at ${new Date().toISOString()}`);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[276]++;
    res.json({
      message: 'Password reset successfully',
      messageBn: '  ',
      timestamp: new Date().toISOString()
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_wmf6zey3v().s[277]++;
    console.error('Reset password error:', error);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[278]++;
    res.status(500).json({
      error: 'Password reset failed',
      message: process.env.NODE_ENV === 'development' ?
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[81][0]++, error.message) :
      /* istanbul ignore next */
