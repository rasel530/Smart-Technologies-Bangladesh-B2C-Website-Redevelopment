// Refresh token endpoint
/* istanbul ignore next */
cov_wmf6zey3v().s[98]++;
router.post('/refresh', async (req, res) => {
  /* istanbul ignore next */
  cov_wmf6zey3v().f[4]++;
  cov_wmf6zey3v().s[99]++;
  try {
    const {
      token
    } =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[100]++, req.body);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[101]++;
    if (!token) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[35][0]++;
      cov_wmf6zey3v().s[102]++;
      return res.status(401).json({
        error: 'Token required',
        messageBn: ' '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[35][1]++;
    }

    // Verify token
    const jwtSecretRefresh =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[103]++, process.env.JWT_SECRET);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[104]++;
    if (!jwtSecretRefresh) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[36][0]++;
      cov_wmf6zey3v().s[105]++;
      throw new Error('JWT_SECRET environment variable is required');
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[36][1]++;
    }
    const decoded =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[106]++, jwt.verify(token, jwtSecretRefresh));

    // Get user info
    const user =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[107]++, await prisma.user.findUnique({
      where: {
        id: decoded.userId
      },
      select: {
        id: true,
        email: true,
        firstName: true,
        lastName: true,
        role: true,
        status: true
      }
    }));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[108]++;
    if (!user) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[37][0]++;
      cov_wmf6zey3v().s[109]++;
      return res.status(401).json({
        error: 'User not found',
        messageBn: '  '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[37][1]++;
    }

    // Generate new token
    const jwtSecretNew =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[110]++, process.env.JWT_SECRET);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[111]++;
    if (!jwtSecretNew) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[38][0]++;
      cov_wmf6zey3v().s[112]++;
      throw new Error('JWT_SECRET environment variable is required');
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[38][1]++;
    }
    const newToken =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[113]++, jwt.sign({
      userId: user.id,
      email: user.email,
      role: user.role
    }, jwtSecretNew, {
      expiresIn: '7d'
    }));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[114]++;
    res.json({
      message: 'Token refreshed successfully',
      messageBn: '  ',
      user,
      token: newToken
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_wmf6zey3v().s[115]++;
    console.error('Token refresh error:', error);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[116]++;
    res.status(401).json({
      error: 'Invalid token',
      message: process.env.NODE_ENV === 'development' ?
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[39][0]++, error.message) :
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[39][1]++, 'Token expired or invalid'),
      messageBn: '   '
    });
  }
});

// Email verification endpoint
/* istanbul ignore next */
cov_wmf6zey3v().s[117]++;
router.post('/verify-email', [body('token').notEmpty().trim()], handleValidationErrors, async (req, res) => {
  /* istanbul ignore next */
  cov_wmf6zey3v().f[5]++;
  cov_wmf6zey3v().s[118]++;
  try {
    const {
      token
    } =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[119]++, req.body);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[120]++;
    if (!token) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[40][0]++;
      cov_wmf6zey3v().s[121]++;
      return res.status(400).json({
        error: 'Token required',
        message: 'Verification token is required',
        messageBn: '  '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[40][1]++;
    }

    // Find verification token
    const verificationToken =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[122]++, await prisma.emailVerificationToken.findUnique({
      where: {
        token
      },
      include: {
        user: true
      }
    }));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[123]++;
    if (!verificationToken) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[41][0]++;
      cov_wmf6zey3v().s[124]++;
      return res.status(400).json({
        error: 'Invalid token',
        message: 'Verification token is invalid or has been used',
        messageBn: '     '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[41][1]++;
    }

    // Check if token has expired
    cov_wmf6zey3v().s[125]++;
    if (verificationToken.expiresAt < new Date()) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[42][0]++;
      cov_wmf6zey3v().s[126]++;
      // Delete expired token
      await prisma.emailVerificationToken.delete({
        where: {
          id: verificationToken.id
        }
      });
      /* istanbul ignore next */
      cov_wmf6zey3v().s[127]++;
      return res.status(400).json({
        error: 'Token expired',
        message: 'Verification token has expired. Please request a new one.',
        messageBn: '       '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[42][1]++;
    }

    // Check if user is already verified
    cov_wmf6zey3v().s[128]++;
    if (verificationToken.user.emailVerified) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[43][0]++;
      cov_wmf6zey3v().s[129]++;
      return res.status(400).json({
        error: 'Already verified',
        message: 'Email has already been verified',
        messageBn: '   '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[43][1]++;
    }

    // Update user status and email verification timestamp
    const updatedUser =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[130]++, await prisma.user.update({
      where: {
        id: verificationToken.userId
      },
      data: {
        status: 'ACTIVE',
        emailVerified: new Date()
      },
      select: {
        id: true,
        email: true,
        firstName: true,
        lastName: true,
        role: true,
        status: true,
        emailVerified: true
      }
    }));

    // Delete verification token
    /* istanbul ignore next */
    cov_wmf6zey3v().s[131]++;
    await prisma.emailVerificationToken.delete({
      where: {
        id: verificationToken.id
      }
    });

    // Send welcome email
    /* istanbul ignore next */
    cov_wmf6zey3v().s[132]++;
    await emailService.sendWelcomeEmail(verificationToken.user.email, `${verificationToken.user.firstName} ${verificationToken.user.lastName}`);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[133]++;
    res.json({
      message: 'Email verified successfully',
      messageBn: '   ',
      user: updatedUser
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_wmf6zey3v().s[134]++;
    console.error('Email verification error:', error);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[135]++;
    res.status(500).json({
      error: 'Verification failed',
      message: process.env.NODE_ENV === 'development' ?
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[44][0]++, error.message) :
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[44][1]++, 'Internal server error'),
      messageBn: '  '
    });
  }
});

// Resend verification email endpoint
/* istanbul ignore next */
cov_wmf6zey3v().s[136]++;
router.post('/resend-verification', [body('email').isEmail().normalizeEmail()], handleValidationErrors, async (req, res) => {
  /* istanbul ignore next */
  cov_wmf6zey3v().f[6]++;
  cov_wmf6zey3v().s[137]++;
  try {
    const {
      email
    } =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[138]++, req.body);

    // Find user
    const user =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[139]++, await prisma.user.findUnique({
      where: {
        email
      },
      include: {
        emailVerificationTokens: {
          where: {
            expiresAt: {
              gt: new Date()
            }
          }
        }
      }
    }));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[140]++;
    if (!user) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[45][0]++;
      cov_wmf6zey3v().s[141]++;
      return res.status(404).json({
        error: 'User not found',
        message: 'No account found with this email address',
        messageBn: '      '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[45][1]++;
    }

    // Check if email is already verified
    cov_wmf6zey3v().s[142]++;
    if (user.emailVerified) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[46][0]++;
      cov_wmf6zey3v().s[143]++;
      return res.status(400).json({
        error: 'Already verified',
        message: 'Email has already been verified',
        messageBn: '   '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[46][1]++;
    }

    // Check if there's already a recent verification token (within 5 minutes)
    const recentToken =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[144]++, user.emailVerificationTokens[0]);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[145]++;
    if (
    /* istanbul ignore next */
    (cov_wmf6zey3v().b[48][0]++, recentToken) &&
    /* istanbul ignore next */
    (cov_wmf6zey3v().b[48][1]++, Date.now() - recentToken.createdAt.getTime() < 5 * 60 * 1000)) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[47][0]++;
      cov_wmf6zey3v().s[146]++;
      return res.status(429).json({
        error: 'Too many requests',
        message: 'Please wait before requesting another verification email',
        messageBn: '       '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[47][1]++;
    }

    // Delete any existing tokens for this user
    cov_wmf6zey3v().s[147]++;
    await prisma.emailVerificationToken.deleteMany({
      where: {
        userId: user.id
      }
    });

    // Generate new verification token
    const verificationToken =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[148]++, emailService.generateVerificationToken());
    const expiresAt =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[149]++, new Date(Date.now() + 24 * 60 * 60 * 1000)); // 24 hours

    // Save new verification token
    /* istanbul ignore next */
    cov_wmf6zey3v().s[150]++;
    await prisma.emailVerificationToken.create({
      data: {
        userId: user.id,
        token: verificationToken,
        expiresAt
      }
    });

    // Send verification email
    const emailResult =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[151]++, await emailService.sendVerificationEmail(email, `${user.firstName} ${user.lastName}`, verificationToken));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[152]++;
    if (!emailResult.success) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[49][0]++;
      cov_wmf6zey3v().s[153]++;
      return res.status(500).json({
        error: 'Failed to send email',
        message: 'Unable to send verification email. Please try again later.',
        messageBn: '       '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[49][1]++;
    }
    cov_wmf6zey3v().s[154]++;
    res.json({
      message: 'Verification email sent successfully',
      messageBn: '    ',
      email: email
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_wmf6zey3v().s[155]++;
    console.error('Resend verification error:', error);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[156]++;
    res.status(500).json({
      error: 'Failed to resend verification',
      message: process.env.NODE_ENV === 'development' ?
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[50][0]++, error.message) :
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[50][1]++, 'Internal server error'),
      messageBn: '    '
    });
  }
});

// Send OTP endpoint
/* istanbul ignore next */
cov_wmf6zey3v().s[157]++;
router.post('/send-otp', [body('phone').notEmpty().trim()], handleValidationErrors, async (req, res) => {
  /* istanbul ignore next */
  cov_wmf6zey3v().f[7]++;
  cov_wmf6zey3v().s[158]++;
  try {
    const {
      phone
    } =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[159]++, req.body);

    // Validate phone number with enhanced service
    const phoneValidation =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[160]++, phoneValidationService.validateForUseCase(phone, 'otp'));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[161]++;
    if (!phoneValidation.isValid) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[51][0]++;
      cov_wmf6zey3v().s[162]++;
      return res.status(400).json({
        error: 'Invalid phone format',
        message: phoneValidation.error,
        messageBn: phoneValidation.errorBn,
        code: phoneValidation.code
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[51][1]++;
    }
    const normalizedPhone =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[163]++, phoneValidation.normalizedPhone);

    // Check if phone is already verified
    const isVerified =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[164]++, await otpService.isPhoneVerified(normalizedPhone));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[165]++;
    if (isVerified) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[52][0]++;
      cov_wmf6zey3v().s[166]++;
      return res.status(400).json({
        error: 'Phone already verified',
        message: 'This phone number is already registered and verified',
        messageBn: '       '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[52][1]++;
    }

    // Generate and send OTP
    const otpResult =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[167]++, await otpService.generatePhoneOTP(normalizedPhone));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[168]++;
    if (!otpResult.success) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[53][0]++;
      cov_wmf6zey3v().s[169]++;
      return res.status(400).json({
        error: 'Failed to send OTP',
        message: otpResult.error,
        messageBn: otpResult.errorBn,
        code: otpResult.code
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[53][1]++;
    }
    cov_wmf6zey3v().s[170]++;
    res.json({
      message: 'OTP sent successfully',
      messageBn: 'OTP   ',
      phone: normalizedPhone,
      operator: otpResult.operator,
      operatorDetails: otpResult.operatorDetails,
      expiresAt: otpResult.expiresAt,
      mock:
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[54][0]++, otpResult.mock) ||
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[54][1]++, false)
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_wmf6zey3v().s[171]++;
    console.error('Send OTP error:', error);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[172]++;
    res.status(500).json({
      error: 'Failed to send OTP',
      message: process.env.NODE_ENV === 'development' ?
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[55][0]++, error.message) :
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[55][1]++, 'Internal server error'),
      messageBn: 'OTP   '
    });
  }
