            description: info.description,
            normalizedPhone: phone,
            examples: info.examples,
            metadata: {
              length: phone.length,
              category: type
            }
          };
        } else
        /* istanbul ignore next */
        {
          cov_33nctabpi().b[23][1]++;
        }
      }
    }
    /* istanbul ignore next */
    cov_33nctabpi().s[56]++;
