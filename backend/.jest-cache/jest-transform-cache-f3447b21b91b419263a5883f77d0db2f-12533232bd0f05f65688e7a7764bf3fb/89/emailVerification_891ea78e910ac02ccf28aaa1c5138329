          id: true,
          emailVerified: true,
          status: true
        }
      }));
      /* istanbul ignore next */
      cov_1tp4860vs2().s[61]++;
      return /* istanbul ignore next */(cov_1tp4860vs2().b[21][0]++, user) &&
      /* istanbul ignore next */
      (cov_1tp4860vs2().b[21][1]++, user.emailVerified) &&
      /* istanbul ignore next */
      (cov_1tp4860vs2().b[21][2]++, user.status !== 'PENDING');
    } catch (error) {
      /* istanbul ignore next */
      cov_1tp4860vs2().s[62]++;
      this.logger.error('Error checking email verification status', error.message, {
        userId
      });
      /* istanbul ignore next */
      cov_1tp4860vs2().s[63]++;
      return false;
    }
  }

  // Helper method to get user verification status
  async getUserVerificationStatus(userId) {
    /* istanbul ignore next */
    cov_1tp4860vs2().f[9]++;
    cov_1tp4860vs2().s[64]++;
    try {
      const user =
      /* istanbul ignore next */
      (cov_1tp4860vs2().s[65]++, await this.prisma.user.findUnique({
        where: {
          id: userId
        },
        select: {
          id: true,
          email: true,
          emailVerified: true,
          status: true,
          createdAt: true
        }
      }));
      /* istanbul ignore next */
      cov_1tp4860vs2().s[66]++;
      if (!user) {
        /* istanbul ignore next */
        cov_1tp4860vs2().b[22][0]++;
        cov_1tp4860vs2().s[67]++;
        return {
          verified: false,
          status: 'NOT_FOUND',
          message: 'User not found'
        };
      } else
      /* istanbul ignore next */
      {
        cov_1tp4860vs2().b[22][1]++;
      }
      cov_1tp4860vs2().s[68]++;
      return {
        verified:
        /* istanbul ignore next */
        (cov_1tp4860vs2().b[23][0]++, !!user.emailVerified) &&
        /* istanbul ignore next */
        (cov_1tp4860vs2().b[23][1]++, user.status !== 'PENDING'),
        status: user.status,
        emailVerified: user.emailVerified,
        email: user.email,
        createdAt: user.createdAt
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_1tp4860vs2().s[69]++;
      this.logger.error('Error getting user verification status', error.message, {
        userId
      });
      /* istanbul ignore next */
      cov_1tp4860vs2().s[70]++;
      return {
        verified: false,
        status: 'ERROR',
        message: 'Unable to check verification status'
      };
    }
  }
}

