        (cov_2f6xv8am1g().s[25]++, await this.loginSecurityService.calculateProgressiveDelay(identifier, ip));
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[26]++;
        if (delay > 0) {
          /* istanbul ignore next */
          cov_2f6xv8am1g().b[5][0]++;
          cov_2f6xv8am1g().s[27]++;
          this.logger.logSecurity('Progressive Delay Applied', null, {
            identifier,
            ip,
            delay,
            userAgent
          });

          // Apply delay before proceeding
          /* istanbul ignore next */
          cov_2f6xv8am1g().s[28]++;
          await new Promise(resolve => {
            /* istanbul ignore next */
            cov_2f6xv8am1g().f[3]++;
            cov_2f6xv8am1g().s[29]++;
            return setTimeout(resolve, delay);
          });
        } else
        /* istanbul ignore next */
        {
          cov_2f6xv8am1g().b[5][1]++;
        }

        // Add security headers to response
        cov_2f6xv8am1g().s[30]++;
        this.addSecurityHeaders(res, {
          userLocked: userLockout.isLocked,
          ipBlocked: ipBlock.isBlocked,
          captchaRequired,
          suspiciousActivity: suspiciousCheck.isSuspicious,
          progressiveDelay: delay
        });

        // Attach security context to request for use in login route
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[31]++;
        req.securityContext = {
          userLockout,
          ipBlock,
          suspiciousCheck,
          captchaRequired,
          progressiveDelay: delay,
          deviceFingerprint: this.loginSecurityService.generateDeviceFingerprint(req)
        };
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[32]++;
        next();
      } catch (error) {
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[33]++;
        this.logger.error('Login security middleware error', error.message, {
          ip: req.ip,
          userAgent: req.get('User-Agent'),
          identifier: req.body.identifier
        });

        // Fail open - allow request but log error
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[34]++;
        next();
      }
    };
  }

  // Get attempt count for response
  async getAttemptCount(identifier, ip) {
    /* istanbul ignore next */
    cov_2f6xv8am1g().f[4]++;
    cov_2f6xv8am1g().s[35]++;
    try {
      const stats =
      /* istanbul ignore next */
      (cov_2f6xv8am1g().s[36]++, await this.loginSecurityService.getLoginAttemptStats(identifier, ip));
      /* istanbul ignore next */
      cov_2f6xv8am1g().s[37]++;
      return {
        userAttempts: stats.userAttempts,
        ipAttempts: stats.ipAttempts
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_2f6xv8am1g().s[38]++;
      return {
        userAttempts: 0,
        ipAttempts: 0
      };
    }
  }

  // Add security headers to response
  addSecurityHeaders(res, securityContext) {
    /* istanbul ignore next */
    cov_2f6xv8am1g().f[5]++;
    cov_2f6xv8am1g().s[39]++;
    // Add security-related headers
    res.set({
      'X-Login-Security-Enabled': 'true',
      'X-User-Locked': securityContext.userLocked.toString(),
      'X-IP-Blocked': securityContext.ipBlocked.toString(),
      'X-Captcha-Required': securityContext.captchaRequired.toString(),
      'X-Suspicious-Activity': securityContext.suspiciousActivity.toString(),
      'X-Progressive-Delay': securityContext.progressiveDelay.toString(),
      'X-Security-Timestamp': new Date().toISOString()
    });
  }

  // Record successful login (clears failed attempts)
  recordSuccessfulLogin(req, user) {
    /* istanbul ignore next */
    cov_2f6xv8am1g().f[6]++;
    cov_2f6xv8am1g().s[40]++;
    return async (req, res, next) => {
      /* istanbul ignore next */
      cov_2f6xv8am1g().f[7]++;
      cov_2f6xv8am1g().s[41]++;
      try {
        const {
          identifier
        } =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[42]++, req.body);
        const ip =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[43]++, req.ip);
        const userAgent =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[44]++, req.get('User-Agent'));

        // Clear failed attempts after successful login
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[45]++;
        await this.loginSecurityService.clearFailedAttempts(identifier, ip);
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[46]++;
        this.logger.logSecurity('Successful Login', user.id, {
          identifier,
          ip,
          userAgent,
          userId: user.id,
          timestamp: new Date().toISOString()
        });

        // Add success headers
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[47]++;
        res.set({
          'X-Login-Status': 'success',
          'X-Security-Cleared': 'true',
          'X-Login-Timestamp': new Date().toISOString()
        });
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[48]++;
        next();
      } catch (error) {
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[49]++;
        this.logger.error('Error recording successful login', error.message);
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[50]++;
        next();
      }
    };
  }

  // Record failed login attempt
  recordFailedLogin() {
    /* istanbul ignore next */
    cov_2f6xv8am1g().f[8]++;
    cov_2f6xv8am1g().s[51]++;
    return async (req, res, next) => {
      /* istanbul ignore next */
      cov_2f6xv8am1g().f[9]++;
      cov_2f6xv8am1g().s[52]++;
      try {
        const {
          identifier
        } =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[53]++, req.body);
        const ip =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[54]++, req.ip);
        const userAgent =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[55]++, req.get('User-Agent'));

        // Record failed attempt with reason
        const reason =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[56]++,
        /* istanbul ignore next */
        (cov_2f6xv8am1g().b[6][0]++, req.authError) ||
        /* istanbul ignore next */
        (cov_2f6xv8am1g().b[6][1]++, 'invalid_credentials'));
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[57]++;
        await this.loginSecurityService.recordFailedAttempt(identifier, ip, userAgent, reason);

        // Add failure headers
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[58]++;
        res.set({
          'X-Login-Status': 'failed',
          'X-Failure-Reason': reason,
          'X-Login-Timestamp': new Date().toISOString()
        });
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[59]++;
        next();
      } catch (error) {
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[60]++;
        this.logger.error('Error recording failed login', error.message);
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[61]++;
        next();
      }
    };
  }

  // Rate limiting middleware specifically for login endpoints
  loginRateLimit(options =
  /* istanbul ignore next */
  (cov_2f6xv8am1g().b[7][0]++, {})) {
    /* istanbul ignore next */
    cov_2f6xv8am1g().f[10]++;
    cov_2f6xv8am1g().s[62]++;
    return async (req, res, next) => {
      /* istanbul ignore next */
      cov_2f6xv8am1g().f[11]++;
      cov_2f6xv8am1g().s[63]++;
      try {
        const config =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[64]++, {
          windowMs:
          /* istanbul ignore next */
          (cov_2f6xv8am1g().b[8][0]++, options.windowMs) ||
          /* istanbul ignore next */
          (cov_2f6xv8am1g().b[8][1]++, 15 * 60 * 1000),
          // 15 minutes
          max:
          /* istanbul ignore next */
          (cov_2f6xv8am1g().b[9][0]++, options.max) ||
          /* istanbul ignore next */
          (cov_2f6xv8am1g().b[9][1]++, 10),
          // 10 login attempts per window
          message:
          /* istanbul ignore next */
          (cov_2f6xv8am1g().b[10][0]++, options.message) ||
          /* istanbul ignore next */
          (cov_2f6xv8am1g().b[10][1]++, 'Too many login attempts, please try again later.'),
          skipSuccessfulRequests: options.skipSuccessfulRequests !== false
        });
        const ip =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[65]++, req.ip);
        const key =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[66]++, `login_rate_limit:${ip}`);
        const now =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[67]++, Date.now());
        const windowStart =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[68]++, now - config.windowMs);
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[69]++;
        if (this.loginSecurityService.redis) {
          /* istanbul ignore next */
          cov_2f6xv8am1g().b[11][0]++;
          // Use Redis for distributed rate limiting
          const pipeline =
          /* istanbul ignore next */
          (cov_2f6xv8am1g().s[70]++, this.loginSecurityService.redis.pipeline());

          // Remove old entries
          /* istanbul ignore next */
          cov_2f6xv8am1g().s[71]++;
          pipeline.zRemRangeByScore(key, 0, windowStart);

          // Add current request
          /* istanbul ignore next */
          cov_2f6xv8am1g().s[72]++;
          pipeline.zAdd(key, [{
            score: now,
            value: `${now}-${Math.random()}`
          }]);

          // Get current count
          /* istanbul ignore next */
          cov_2f6xv8am1g().s[73]++;
          pipeline.zCard(key);

          // Set expiration
          /* istanbul ignore next */
          cov_2f6xv8am1g().s[74]++;
          pipeline.expire(key, Math.ceil(config.windowMs / 1000));
          const results =
          /* istanbul ignore next */
          (cov_2f6xv8am1g().s[75]++, await pipeline.exec());
          const currentCount =
          /* istanbul ignore next */
          (cov_2f6xv8am1g().s[76]++, results[2].reply);
          /* istanbul ignore next */
          cov_2f6xv8am1g().s[77]++;
          if (currentCount > config.max) {
            /* istanbul ignore next */
            cov_2f6xv8am1g().b[12][0]++;
            cov_2f6xv8am1g().s[78]++;
            this.logger.logSecurity('Login Rate Limit Exceeded', null, {
              ip,
              userAgent: req.get('User-Agent'),
              count: currentCount,
              limit: config.max,
              windowMs: config.windowMs,
              path: req.originalUrl
            });
            /* istanbul ignore next */
            cov_2f6xv8am1g().s[79]++;
            return res.status(429).json({
              error: 'Too many login attempts',
              message: config.message,
              messageBn: 'অত্যাধিক লগইন চেষ্টা, অনুগ্রহ পরে চেষ্টা করুন',
              retryAfter: Math.ceil(config.windowMs / 1000),
              rateLimitDetails: {
                currentCount,
                limit: config.max,
                windowMs: config.windowMs,
                resetTime: new Date(now + config.windowMs).toISOString()
              }
            });
          } else
          /* istanbul ignore next */
          {
            cov_2f6xv8am1g().b[12][1]++;
          }

          // Add rate limit headers
          cov_2f6xv8am1g().s[80]++;
          res.set({
            'X-Login-RateLimit-Limit': config.max,
            'X-Login-RateLimit-Remaining': Math.max(0, config.max - currentCount),
            'X-Login-RateLimit-Reset': new Date(now + config.windowMs).toISOString()
          });
        } else
        /* istanbul ignore next */
        {
          cov_2f6xv8am1g().b[11][1]++;
        }
        cov_2f6xv8am1g().s[81]++;
        next();
      } catch (error) {
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[82]++;
        this.logger.error('Login rate limiting error', error.message);
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[83]++;
        next(); // Fail open
      }
    };
  }

  // Enhanced security validation middleware
  validateSecurity() {
    /* istanbul ignore next */
    cov_2f6xv8am1g().f[12]++;
    cov_2f6xv8am1g().s[84]++;
    return (req, res, next) => {
      /* istanbul ignore next */
      cov_2f6xv8am1g().f[13]++;
      cov_2f6xv8am1g().s[85]++;
      try {
        const securityContext =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[86]++, req.securityContext);
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[87]++;
        if (!securityContext) {
          /* istanbul ignore next */
          cov_2f6xv8am1g().b[13][0]++;
          cov_2f6xv8am1g().s[88]++;
          return next();
        } else
        /* istanbul ignore next */
        {
          cov_2f6xv8am1g().b[13][1]++;
        }

        // Additional validation based on security context
        const validationErrors =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[89]++, []);

        // Check for high-risk patterns
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[90]++;
        if (securityContext.suspiciousCheck.riskScore >= 5) {
          /* istanbul ignore next */
          cov_2f6xv8am1g().b[14][0]++;
          cov_2f6xv8am1g().s[91]++;
          validationErrors.push('High risk activity detected');
        } else
        /* istanbul ignore next */
        {
          cov_2f6xv8am1g().b[14][1]++;
        }

        // Check for multiple security violations
        let violationCount =
        /* istanbul ignore next */
        (cov_2f6xv8am1g().s[92]++, 0);
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[93]++;
        if (securityContext.userLocked.isLocked) {
          /* istanbul ignore next */
          cov_2f6xv8am1g().b[15][0]++;
          cov_2f6xv8am1g().s[94]++;
          violationCount++;
        } else
        /* istanbul ignore next */
        {
          cov_2f6xv8am1g().b[15][1]++;
        }
        cov_2f6xv8am1g().s[95]++;
        if (securityContext.ipBlock.isBlocked) {
          /* istanbul ignore next */
          cov_2f6xv8am1g().b[16][0]++;
          cov_2f6xv8am1g().s[96]++;
          violationCount++;
        } else
        /* istanbul ignore next */
        {
          cov_2f6xv8am1g().b[16][1]++;
        }
        cov_2f6xv8am1g().s[97]++;
        if (securityContext.captchaRequired) {
          /* istanbul ignore next */
          cov_2f6xv8am1g().b[17][0]++;
          cov_2f6xv8am1g().s[98]++;
          violationCount++;
        } else
        /* istanbul ignore next */
        {
          cov_2f6xv8am1g().b[17][1]++;
        }
        cov_2f6xv8am1g().s[99]++;
        if (securityContext.suspiciousCheck.isSuspicious) {
          /* istanbul ignore next */
          cov_2f6xv8am1g().b[18][0]++;
          cov_2f6xv8am1g().s[100]++;
          violationCount++;
        } else
        /* istanbul ignore next */
        {
          cov_2f6xv8am1g().b[18][1]++;
        }
        cov_2f6xv8am1g().s[101]++;
        if (violationCount >= 2) {
          /* istanbul ignore next */
          cov_2f6xv8am1g().b[19][0]++;
          cov_2f6xv8am1g().s[102]++;
          validationErrors.push('Multiple security violations detected');
        } else
        /* istanbul ignore next */
        {
          cov_2f6xv8am1g().b[19][1]++;
        }
        cov_2f6xv8am1g().s[103]++;
        if (validationErrors.length > 0) {
          /* istanbul ignore next */
          cov_2f6xv8am1g().b[20][0]++;
          cov_2f6xv8am1g().s[104]++;
          this.logger.logSecurity('Security Validation Failed', null, {
            ip: req.ip,
            userAgent: req.get('User-Agent'),
            identifier: req.body.identifier,
            validationErrors,
            securityContext
          });
          /* istanbul ignore next */
          cov_2f6xv8am1g().s[105]++;
          return res.status(403).json({
            error: 'Security validation failed',
            message: 'Your request has been blocked due to security concerns',
            messageBn: 'নিরাপত্তিগিয় উদ্বেগ আপনার অনুরোধ ব্লক করা হয়েছে',
            validationErrors,
            securityContext: {
              riskScore: securityContext.suspiciousCheck.riskScore,
              violations: violationCount
            }
          });
        } else
        /* istanbul ignore next */
        {
          cov_2f6xv8am1g().b[20][1]++;
        }
        cov_2f6xv8am1g().s[106]++;
        next();
      } catch (error) {
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[107]++;
        this.logger.error('Security validation error', error.message);
        /* istanbul ignore next */
        cov_2f6xv8am1g().s[108]++;
        next();
      }
    };
  }
}

