    cov_wmf6zey3v().s[201]++;
    console.error('Resend OTP error:', error);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[202]++;
    res.status(500).json({
      error: 'Failed to resend OTP',
      message: process.env.NODE_ENV === 'development' ?
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[64][0]++, error.message) :
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[64][1]++, 'Internal server error'),
