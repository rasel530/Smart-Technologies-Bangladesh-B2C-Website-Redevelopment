  // Send password reset email
  async sendPasswordResetEmail(userEmail, userName, temporaryPassword, resetToken) {
    /* istanbul ignore next */
    cov_x0dvunhp7().f[11]++;
    cov_x0dvunhp7().s[55]++;
    try {
      /* istanbul ignore next */
      cov_x0dvunhp7().s[56]++;
      if (!this.transporter) {
        /* istanbul ignore next */
        cov_x0dvunhp7().b[11][0]++;
        cov_x0dvunhp7().s[57]++;
        throw new Error('Email service not available');
      } else
      /* istanbul ignore next */
      {
        cov_x0dvunhp7().b[11][1]++;
      }
      const emailConfig =
      /* istanbul ignore next */
      (cov_x0dvunhp7().s[58]++, this.config.getEmailConfig());
      const emailTemplate =
      /* istanbul ignore next */
      (cov_x0dvunhp7().s[59]++, this.createPasswordResetEmailTemplate(userName, temporaryPassword, resetToken));
      const mailOptions =
      /* istanbul ignore next */
      (cov_x0dvunhp7().s[60]++, {
        from: `"Smart Technologies Bangladesh" <${emailConfig.from}>`,
        to: userEmail,
        subject: emailTemplate.subject,
        html: emailTemplate.html,
        text: emailTemplate.text
      });
      const result =
      /* istanbul ignore next */
      (cov_x0dvunhp7().s[61]++, await this.transporter.sendMail(mailOptions));
      /* istanbul ignore next */
      cov_x0dvunhp7().s[62]++;
      this.logger.info('Password reset email sent successfully', {
        to: userEmail,
        messageId: result.messageId,
        timestamp: new Date().toISOString()
      });
      /* istanbul ignore next */
      cov_x0dvunhp7().s[63]++;
      return {
        success: true,
        messageId: result.messageId,
        timestamp: new Date().toISOString()
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_x0dvunhp7().s[64]++;
      this.logger.error('Failed to send password reset email', error.message, {
        to: userEmail,
        errorType: error.name
      });
      /* istanbul ignore next */
      cov_x0dvunhp7().s[65]++;
      return {
        success: false,
        error: error.message,
        timestamp: new Date().toISOString()
      };
    }
  }
