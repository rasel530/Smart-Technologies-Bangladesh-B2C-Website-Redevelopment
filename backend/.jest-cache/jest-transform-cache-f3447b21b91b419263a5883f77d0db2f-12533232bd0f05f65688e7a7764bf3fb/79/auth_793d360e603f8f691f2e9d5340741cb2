// Verify OTP endpoint
/* istanbul ignore next */
cov_wmf6zey3v().s[173]++;
router.post('/verify-otp', [body('phone').notEmpty().trim(), body('otp').notEmpty().trim().isLength({
  min: 6,
  max: 6
})], handleValidationErrors, async (req, res) => {
  /* istanbul ignore next */
  cov_wmf6zey3v().f[8]++;
  cov_wmf6zey3v().s[174]++;
  try {
    const {
      phone,
      otp
    } =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[175]++, req.body);

    // Validate phone number with enhanced service
    const phoneValidation =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[176]++, phoneValidationService.validateForUseCase(phone, 'otp'));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[177]++;
    if (!phoneValidation.isValid) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[56][0]++;
      cov_wmf6zey3v().s[178]++;
      return res.status(400).json({
        error: 'Invalid phone format',
        message: phoneValidation.error,
        messageBn: phoneValidation.errorBn,
        code: phoneValidation.code
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[56][1]++;
    }
    const normalizedPhone =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[179]++, phoneValidation.normalizedPhone);

    // Verify OTP
    const verifyResult =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[180]++, await otpService.verifyPhoneOTP(normalizedPhone, otp));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[181]++;
    if (!verifyResult.success) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[57][0]++;
      cov_wmf6zey3v().s[182]++;
      return res.status(400).json({
        error: 'OTP verification failed',
        message: verifyResult.error,
        messageBn: verifyResult.errorBn,
        code: verifyResult.code
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[57][1]++;
    }

    // If OTP was for registration, find and update user
    cov_wmf6zey3v().s[183]++;
    if (verifyResult.userId) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[58][0]++;
      const user =
      /* istanbul ignore next */
      (cov_wmf6zey3v().s[184]++, await prisma.user.findUnique({
        where: {
          id: verifyResult.userId
        },
        select: {
          id: true,
          email: true,
          firstName: true,
          lastName: true,
          phone: true,
          role: true,
          status: true,
          phoneVerified: true
        }
      }));
      /* istanbul ignore next */
      cov_wmf6zey3v().s[185]++;
      if (user) {
        /* istanbul ignore next */
        cov_wmf6zey3v().b[59][0]++;
        cov_wmf6zey3v().s[186]++;
        return res.json({
          message: 'Phone verified successfully. Account is now active.',
          messageBn: '      ',
          user,
          verifiedAt: verifyResult.verifiedAt
        });
      } else
      /* istanbul ignore next */
      {
        cov_wmf6zey3v().b[59][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[58][1]++;
    }
    cov_wmf6zey3v().s[187]++;
    res.json({
      message: 'OTP verified successfully',
      messageBn: 'OTP   ',
      phone: normalizedPhone,
      verifiedAt: verifyResult.verifiedAt
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_wmf6zey3v().s[188]++;
    console.error('Verify OTP error:', error);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[189]++;
    res.status(500).json({
      error: 'OTP verification failed',
      message: process.env.NODE_ENV === 'development' ?
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[60][0]++, error.message) :
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[60][1]++, 'Internal server error'),
      messageBn: 'OTP   '
    });
  }
});

