// Request password reset endpoint
/* istanbul ignore next */
cov_wmf6zey3v().s[232]++;
router.post('/forgot-password', [body('email').isEmail().normalizeEmail()], handleValidationErrors, async (req, res) => {
  /* istanbul ignore next */
  cov_wmf6zey3v().f[11]++;
  cov_wmf6zey3v().s[233]++;
  try {
    const {
      email
    } =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[234]++, req.body);

    // Find user
    const user =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[235]++, await prisma.user.findUnique({
      where: {
        email
      }
    }));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[236]++;
    if (!user) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[72][0]++;
      cov_wmf6zey3v().s[237]++;
      // Don't reveal if user exists or not for security
      return res.json({
        message: 'If an account with that email exists, a password reset link has been sent',
        messageBn: '      ,      '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[72][1]++;
    }

    // Generate reset token (you might want to use a separate token service)
    const resetToken =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[238]++, emailService.generateVerificationToken());
    const expiresAt =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[239]++, new Date(Date.now() + 1 * 60 * 60 * 1000)); // 1 hour

    // Save reset token (using email verification token table for simplicity)
    /* istanbul ignore next */
    cov_wmf6zey3v().s[240]++;
    await prisma.emailVerificationToken.create({
      data: {
        userId: user.id,
        token: resetToken,
        expiresAt
      }
    });

    // Generate temporary strong password
    const temporaryPassword =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[241]++, passwordService.generateStrongPassword(12));
    const hashedTempPassword =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[242]++, await passwordService.hashPassword(temporaryPassword));

    // Update user with temporary password
    /* istanbul ignore next */
    cov_wmf6zey3v().s[243]++;
    await prisma.user.update({
      where: {
        id: user.id
      },
      data: {
        password: hashedTempPassword
      }
    });

    // Save temporary password to history
    /* istanbul ignore next */
    cov_wmf6zey3v().s[244]++;
    await passwordService.savePasswordToHistory(user.id, hashedTempPassword);

    // Send reset email with temporary password
    const emailResult =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[245]++, await emailService.sendPasswordResetEmail(email, `${user.firstName} ${user.lastName}`, temporaryPassword, resetToken));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[246]++;
    if (!emailResult.success) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[73][0]++;
      cov_wmf6zey3v().s[247]++;
      return res.status(500).json({
        error: 'Failed to send reset email',
        message: 'Unable to send password reset email. Please try again later.',
        messageBn: '        '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[73][1]++;
    }
    cov_wmf6zey3v().s[248]++;
    res.json({
      message: 'Password reset email sent successfully',
      messageBn: '     ',
      email: email
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_wmf6zey3v().s[249]++;
    console.error('Forgot password error:', error);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[250]++;
    res.status(500).json({
      error: 'Password reset failed',
      message: process.env.NODE_ENV === 'development' ?
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[74][0]++, error.message) :
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[74][1]++, 'Internal server error'),
      messageBn: '   '
    });
  }
});

