  /**
   * Validate phone number for specific use case
   * @param {string} phone - Phone number
   * @param {string} useCase - Use case ('registration', 'otp', 'sms', 'verification')
   * @returns {Object} Validation result with use-case specific rules
   */
  validateForUseCase(phone, useCase) {
    /* istanbul ignore next */
    cov_33nctabpi().f[19]++;
    const baseValidation =
    /* istanbul ignore next */
    (cov_33nctabpi().s[101]++, this.validateBangladeshPhoneNumber(phone, {
      allowLandline:
      /* istanbul ignore next */
      (cov_33nctabpi().b[39][0]++, useCase !== 'otp') &&
      /* istanbul ignore next */
      (cov_33nctabpi().b[39][1]++, useCase !== 'sms'),
      allowMobile: true,
      allowSpecial: false
    }));
    /* istanbul ignore next */
    cov_33nctabpi().s[102]++;
    if (!baseValidation.isValid) {
      /* istanbul ignore next */
      cov_33nctabpi().b[40][0]++;
      cov_33nctabpi().s[103]++;
      return baseValidation;
    } else
    /* istanbul ignore next */
    {
      cov_33nctabpi().b[40][1]++;
    }

    // Use-case specific validations
    cov_33nctabpi().s[104]++;
    switch (useCase) {
      case 'registration':
        /* istanbul ignore next */
        cov_33nctabpi().b[41][0]++;
        cov_33nctabpi().s[105]++;
        return {
          ...baseValidation,
          canReceiveSMS: baseValidation.type === 'mobile',
          canReceiveOTP: baseValidation.type === 'mobile',
          requiresVerification: true
        };
      case 'otp':
        /* istanbul ignore next */
        cov_33nctabpi().b[41][1]++;
      case 'sms':
        /* istanbul ignore next */
        cov_33nctabpi().b[41][2]++;
        cov_33nctabpi().s[106]++;
        if (baseValidation.type !== 'mobile') {
          /* istanbul ignore next */
          cov_33nctabpi().b[42][0]++;
          cov_33nctabpi().s[107]++;
          return {
            isValid: false,
            error: 'Only mobile numbers can receive SMS/OTP',
            errorBn: '   SMS/OTP  ',
            code: 'MOBILE_ONLY'
          };
        } else
        /* istanbul ignore next */
        {
          cov_33nctabpi().b[42][1]++;
        }
        cov_33nctabpi().s[108]++;
        return baseValidation;
      case 'verification':
        /* istanbul ignore next */
        cov_33nctabpi().b[41][3]++;
        cov_33nctabpi().s[109]++;
        return {
          ...baseValidation,
          isVerifiable: true
        };
      default:
        /* istanbul ignore next */
        cov_33nctabpi().b[41][4]++;
        cov_33nctabpi().s[110]++;
        return baseValidation;
    }
  }

  /**
   * Generate validation statistics
   * @param {Array} phoneNumbers - Array of phone numbers to analyze
   * @returns {Object} Statistics about phone numbers
   */
  generateValidationStats(phoneNumbers) {
    /* istanbul ignore next */
    cov_33nctabpi().f[20]++;
    const stats =
    /* istanbul ignore next */
    (cov_33nctabpi().s[111]++, {
      total: phoneNumbers.length,
      valid: 0,
      invalid: 0,
      mobile: 0,
      landline: 0,
      special: 0,
      operators: {},
      areas: {},
      formats: {
        international: 0,
        local: 0,
        country_code: 0
      }
    });
    /* istanbul ignore next */
    cov_33nctabpi().s[112]++;
    phoneNumbers.forEach(phone => {
      /* istanbul ignore next */
      cov_33nctabpi().f[21]++;
      const validation =
      /* istanbul ignore next */
      (cov_33nctabpi().s[113]++, this.validateBangladeshPhoneNumber(phone));
      /* istanbul ignore next */
      cov_33nctabpi().s[114]++;
      if (validation.isValid) {
        /* istanbul ignore next */
        cov_33nctabpi().b[43][0]++;
        cov_33nctabpi().s[115]++;
        stats.valid++;
        /* istanbul ignore next */
        cov_33nctabpi().s[116]++;
        stats[validation.type]++;
        /* istanbul ignore next */
        cov_33nctabpi().s[117]++;
        stats.formats[validation.format]++;
        /* istanbul ignore next */
        cov_33nctabpi().s[118]++;
        if (validation.operator) {
          /* istanbul ignore next */
          cov_33nctabpi().b[44][0]++;
          cov_33nctabpi().s[119]++;
          stats.operators[validation.operator] = (
          /* istanbul ignore next */
          (cov_33nctabpi().b[45][0]++, stats.operators[validation.operator]) ||
          /* istanbul ignore next */
          (cov_33nctabpi().b[45][1]++, 0)) + 1;
        } else
        /* istanbul ignore next */
        {
          cov_33nctabpi().b[44][1]++;
        }
        cov_33nctabpi().s[120]++;
        if (validation.areaCode) {
          /* istanbul ignore next */
          cov_33nctabpi().b[46][0]++;
          cov_33nctabpi().s[121]++;
          stats.areas[validation.areaCode] = (
          /* istanbul ignore next */
          (cov_33nctabpi().b[47][0]++, stats.areas[validation.areaCode]) ||
          /* istanbul ignore next */
          (cov_33nctabpi().b[47][1]++, 0)) + 1;
        } else
        /* istanbul ignore next */
        {
          cov_33nctabpi().b[46][1]++;
        }
      } else {
        /* istanbul ignore next */
        cov_33nctabpi().b[43][1]++;
        cov_33nctabpi().s[122]++;
        stats.invalid++;
      }
    });
    /* istanbul ignore next */
    cov_33nctabpi().s[123]++;
    return stats;
  }
}

// Singleton instance
const phoneValidationService =
/* istanbul ignore next */
(cov_33nctabpi().s[124]++, new PhoneValidationService());
/* istanbul ignore next */
cov_33nctabpi().s[125]++;
module.exports = {
  PhoneValidationService,
  phoneValidationService
};
