const handleValidationErrors = (req, res, next) => {
  /* istanbul ignore next */
  cov_wmf6zey3v().f[0]++;
  const errors =
  /* istanbul ignore next */
  (cov_wmf6zey3v().s[14]++, validationResult(req));
  /* istanbul ignore next */
  cov_wmf6zey3v().s[15]++;
  if (!errors.isEmpty()) {
    /* istanbul ignore next */
    cov_wmf6zey3v().b[0][0]++;
    cov_wmf6zey3v().s[16]++;
    return res.status(400).json({
      error: 'Validation failed',
      details: errors.array()
    });
  } else
  /* istanbul ignore next */
  {
    cov_wmf6zey3v().b[0][1]++;
  }
  cov_wmf6zey3v().s[17]++;
  next();
};

// Register endpoint
/* istanbul ignore next */
cov_wmf6zey3v().s[18]++;
router.post('/register', [body('email').optional().isEmail().normalizeEmail(), body('password').isLength({
  min: 8,
  max: 128
}), body('firstName').notEmpty().trim(), body('lastName').notEmpty().trim(), body('phone').notEmpty().trim()], handleValidationErrors, async (req, res) => {
  /* istanbul ignore next */
  cov_wmf6zey3v().f[1]++;
  cov_wmf6zey3v().s[19]++;
  try {
    const {
      email,
      password,
      firstName,
      lastName,
      phone
    } =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[20]++, req.body);

    // Validate that at least email or phone is provided
    /* istanbul ignore next */
    cov_wmf6zey3v().s[21]++;
    if (
    /* istanbul ignore next */
    (cov_wmf6zey3v().b[2][0]++, !email) &&
    /* istanbul ignore next */
    (cov_wmf6zey3v().b[2][1]++, !phone)) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[1][0]++;
      cov_wmf6zey3v().s[22]++;
      return res.status(400).json({
        error: 'Email or phone required',
        message: 'Please provide either email or phone number for registration',
        messageBn: '     '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[1][1]++;
    }

    // Validate password strength
    const userInfo =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[23]++, {
      firstName,
      lastName,
      email,
      phone
    });
    const passwordValidation =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[24]++, passwordService.validatePasswordStrength(password, userInfo));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[25]++;
    if (!passwordValidation.isValid) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[3][0]++;
      cov_wmf6zey3v().s[26]++;
      return res.status(400).json({
        error: 'Password does not meet requirements',
        message: 'Password is too weak or does not meet security requirements',
        messageBn: '    ',
        details: {
          strength: passwordValidation.strength,
          score: passwordValidation.score,
          feedback: passwordValidation.feedback,
          warnings: passwordValidation.warnings,
          suggestions: passwordValidation.suggestions,
          passwordPolicy: passwordService.getPasswordPolicy()
        }
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[3][1]++;
    }

    // Validate email if provided
    cov_wmf6zey3v().s[27]++;
    if (email) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[4][0]++;
      cov_wmf6zey3v().s[28]++;
      if (!emailService.validateEmail(email)) {
        /* istanbul ignore next */
        cov_wmf6zey3v().b[5][0]++;
        cov_wmf6zey3v().s[29]++;
        return res.status(400).json({
          error: 'Invalid email format',
          message: 'Please provide a valid email address',
          messageBn: '   '
        });
      } else
      /* istanbul ignore next */
      {
        cov_wmf6zey3v().b[5][1]++;
      }
      cov_wmf6zey3v().s[30]++;
      if (emailService.isDisposableEmail(email)) {
        /* istanbul ignore next */
        cov_wmf6zey3v().b[6][0]++;
        cov_wmf6zey3v().s[31]++;
        return res.status(400).json({
          error: 'Disposable email not allowed',
          message: 'Please use a permanent email address for registration',
          messageBn: '   ,    '
        });
      } else
      /* istanbul ignore next */
      {
        cov_wmf6zey3v().b[6][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[4][1]++;
    }

    // Validate phone if provided
    let phoneValidation =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[32]++, null);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[33]++;
    if (phone) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[7][0]++;
      cov_wmf6zey3v().s[34]++;
      phoneValidation = phoneValidationService.validateForUseCase(phone, 'registration');
      /* istanbul ignore next */
      cov_wmf6zey3v().s[35]++;
      if (!phoneValidation.isValid) {
        /* istanbul ignore next */
        cov_wmf6zey3v().b[8][0]++;
        cov_wmf6zey3v().s[36]++;
        return res.status(400).json({
          error: 'Invalid phone format',
          message: phoneValidation.error,
          messageBn: phoneValidation.errorBn,
          code: phoneValidation.code
        });
      } else
      /* istanbul ignore next */
      {
        cov_wmf6zey3v().b[8][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[7][1]++;
    }

    // Check if user already exists
    const existingUser =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[37]++, await prisma.user.findFirst({
      where: {
        OR: [...(email ?
        /* istanbul ignore next */
        (cov_wmf6zey3v().b[9][0]++, [{
          email
        }]) :
        /* istanbul ignore next */
        (cov_wmf6zey3v().b[9][1]++, [])), ...(phoneValidation?.isValid ?
        /* istanbul ignore next */
        (cov_wmf6zey3v().b[10][0]++, [{
          phone: phoneValidation.normalizedPhone
        }]) :
        /* istanbul ignore next */
        (cov_wmf6zey3v().b[10][1]++, []))]
      }
    }));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[38]++;
    if (existingUser) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[11][0]++;
      cov_wmf6zey3v().s[39]++;
      return res.status(409).json({
        error: 'User already exists',
        message: 'An account with this email or phone number already exists',
        messageBn: '        ',
        field: existingUser.email === email ?
        /* istanbul ignore next */
        (cov_wmf6zey3v().b[12][0]++, 'email') :
        /* istanbul ignore next */
        (cov_wmf6zey3v().b[12][1]++, 'phone')
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[11][1]++;
    }

    // Hash password using password service
    const hashedPassword =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[40]++, await passwordService.hashPassword(password));

    // Create user with PENDING status
    const user =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[41]++, await prisma.user.create({
      data: {
        email,
        password: hashedPassword,
        firstName,
        lastName,
        phone:
        /* istanbul ignore next */
        (cov_wmf6zey3v().b[13][0]++, phoneValidation?.normalizedPhone) ||
        /* istanbul ignore next */
        (cov_wmf6zey3v().b[13][1]++, phone),
        role: 'CUSTOMER',
        status: 'PENDING'
      },
      select: {
        id: true,
        email: true,
        firstName: true,
        lastName: true,
        phone: true,
        role: true,
        status: true,
        createdAt: true
      }
    }));

    // Save password to history
    /* istanbul ignore next */
    cov_wmf6zey3v().s[42]++;
    await passwordService.savePasswordToHistory(user.id, hashedPassword);

    // Handle verification based on what's provided
    /* istanbul ignore next */
    cov_wmf6zey3v().s[43]++;
    if (email) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[14][0]++;
      // Email verification flow
      const verificationToken =
      /* istanbul ignore next */
      (cov_wmf6zey3v().s[44]++, emailService.generateVerificationToken());
      const expiresAt =
      /* istanbul ignore next */
      (cov_wmf6zey3v().s[45]++, new Date(Date.now() + 24 * 60 * 60 * 1000)); // 24 hours

      // Save verification token to database
      /* istanbul ignore next */
      cov_wmf6zey3v().s[46]++;
      await prisma.emailVerificationToken.create({
        data: {
          userId: user.id,
          token: verificationToken,
          expiresAt
        }
      });

      // Send verification email
      const emailResult =
      /* istanbul ignore next */
      (cov_wmf6zey3v().s[47]++, await emailService.sendVerificationEmail(email, `${firstName} ${lastName}`, verificationToken));
      /* istanbul ignore next */
      cov_wmf6zey3v().s[48]++;
      if (!emailResult.success) {
        /* istanbul ignore next */
        cov_wmf6zey3v().b[15][0]++;
        cov_wmf6zey3v().s[49]++;
        // If email fails, delete user and token for cleanup
        await prisma.emailVerificationToken.delete({
          where: {
            userId: user.id
          }
        });
        /* istanbul ignore next */
        cov_wmf6zey3v().s[50]++;
        await prisma.user.delete({
          where: {
            id: user.id
          }
        });
        /* istanbul ignore next */
        cov_wmf6zey3v().s[51]++;
        return res.status(500).json({
          error: 'Registration failed',
          message: 'Failed to send verification email. Please try again later.',
          messageBn: '       '
        });
      } else
      /* istanbul ignore next */
      {
        cov_wmf6zey3v().b[15][1]++;
      }
      cov_wmf6zey3v().s[52]++;
      res.status(201).json({
        message: 'Registration successful. Please check your email to verify your account.',
        messageBn: '         ',
        user: {
          id: user.id,
          email: user.email,
          firstName: user.firstName,
          lastName: user.lastName,
          role: user.role,
          status: user.status
        },
        requiresEmailVerification: true
      });
    } else {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[14][1]++;
      cov_wmf6zey3v().s[53]++;
      if (phoneValidation?.isValid) {
        /* istanbul ignore next */
        cov_wmf6zey3v().b[16][0]++;
        // Phone verification flow
        const otpResult =
        /* istanbul ignore next */
        (cov_wmf6zey3v().s[54]++, await otpService.generatePhoneOTP(phoneValidation.normalizedPhone, user.id));
        /* istanbul ignore next */
        cov_wmf6zey3v().s[55]++;
        if (!otpResult.success) {
          /* istanbul ignore next */
          cov_wmf6zey3v().b[17][0]++;
          cov_wmf6zey3v().s[56]++;
          // If OTP fails, delete user for cleanup
          await prisma.user.delete({
            where: {
              id: user.id
            }
          });
          /* istanbul ignore next */
          cov_wmf6zey3v().s[57]++;
          return res.status(500).json({
            error: 'Registration failed',
            message:
            /* istanbul ignore next */
            (cov_wmf6zey3v().b[18][0]++, otpResult.error) ||
            /* istanbul ignore next */
            (cov_wmf6zey3v().b[18][1]++, 'Failed to send OTP. Please try again later.'),
            messageBn:
            /* istanbul ignore next */
            (cov_wmf6zey3v().b[19][0]++, otpResult.errorBn) ||
            /* istanbul ignore next */
            (cov_wmf6zey3v().b[19][1]++, 'OTP      ')
          });
        } else
        /* istanbul ignore next */
        {
          cov_wmf6zey3v().b[17][1]++;
        }
        cov_wmf6zey3v().s[58]++;
        res.status(201).json({
          message: 'Registration successful. Please check your phone for OTP verification.',
          messageBn: '    OTP     ',
          user: {
            id: user.id,
            phone: user.phone,
            firstName: user.firstName,
            lastName: user.lastName,
            role: user.role,
            status: user.status
          },
          requiresPhoneVerification: true,
          phone: phoneValidation.normalizedPhone,
          operator: phoneValidation.operator,
          operatorDetails: phoneValidation.operatorDetails
        });
      } else {
        /* istanbul ignore next */
        cov_wmf6zey3v().b[16][1]++;
        cov_wmf6zey3v().s[59]++;
        // This should not happen due to earlier validation
        return res.status(500).json({
          error: 'Registration failed',
          message: 'Invalid registration data',
          messageBn: '  '
        });
      }
    }
  } catch (error) {
    /* istanbul ignore next */
    cov_wmf6zey3v().s[60]++;
    console.error('Registration error:', error);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[61]++;
    res.status(500).json({
      error: 'Registration failed',
      message: process.env.NODE_ENV === 'development' ?
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[20][0]++, error.message) :
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[20][1]++, 'Internal server error'),
      messageBn: '  '
    });
  }
});

// Login endpoint
/* istanbul ignore next */
cov_wmf6zey3v().s[62]++;
router.post('/login', [body('identifier').notEmpty().trim(), body('password').notEmpty()], handleValidationErrors, async (req, res) => {
  /* istanbul ignore next */
  cov_wmf6zey3v().f[2]++;
  cov_wmf6zey3v().s[63]++;
  try {
    const {
      identifier,
      password
    } =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[64]++, req.body);

    // Determine if identifier is email or phone
    const isEmail =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[65]++, identifier.includes('@'));
    let user;
    let loginType;
    /* istanbul ignore next */
    cov_wmf6zey3v().s[66]++;
    if (isEmail) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[21][0]++;
      cov_wmf6zey3v().s[67]++;
      // Validate email format
      if (!emailService.validateEmail(identifier)) {
        /* istanbul ignore next */
        cov_wmf6zey3v().b[22][0]++;
        cov_wmf6zey3v().s[68]++;
        return res.status(400).json({
          error: 'Invalid email format',
          message: 'Please provide a valid email address',
          messageBn: '   '
        });
      } else
      /* istanbul ignore next */
      {
        cov_wmf6zey3v().b[22][1]++;
      }

      // Find user by email
      cov_wmf6zey3v().s[69]++;
      user = await prisma.user.findUnique({
        where: {
          email: identifier
        }
      });
      /* istanbul ignore next */
      cov_wmf6zey3v().s[70]++;
      loginType = 'email';
    } else {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[21][1]++;
      // Validate phone number
      const phoneValidation =
      /* istanbul ignore next */
      (cov_wmf6zey3v().s[71]++, phoneValidationService.validateForUseCase(identifier, 'login'));
      /* istanbul ignore next */
      cov_wmf6zey3v().s[72]++;
      if (!phoneValidation.isValid) {
        /* istanbul ignore next */
        cov_wmf6zey3v().b[23][0]++;
        cov_wmf6zey3v().s[73]++;
        return res.status(400).json({
          error: 'Invalid phone format',
          message: phoneValidation.error,
          messageBn: phoneValidation.errorBn,
          code: phoneValidation.code
        });
      } else
      /* istanbul ignore next */
      {
        cov_wmf6zey3v().b[23][1]++;
      }

      // Find user by phone
      cov_wmf6zey3v().s[74]++;
      user = await prisma.user.findUnique({
        where: {
          phone: phoneValidation.normalizedPhone
        }
      });
      /* istanbul ignore next */
      cov_wmf6zey3v().s[75]++;
      loginType = 'phone';
    }
    /* istanbul ignore next */
    cov_wmf6zey3v().s[76]++;
    if (
    /* istanbul ignore next */
    (cov_wmf6zey3v().b[25][0]++, !user) ||
    /* istanbul ignore next */
    (cov_wmf6zey3v().b[25][1]++, !user.password)) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[24][0]++;
      cov_wmf6zey3v().s[77]++;
      return res.status(401).json({
        error: 'Invalid credentials',
        message: `Invalid ${loginType} or password`,
        messageBn: loginType === 'email' ?
        /* istanbul ignore next */
        (cov_wmf6zey3v().b[26][0]++, '   ') :
        /* istanbul ignore next */
        (cov_wmf6zey3v().b[26][1]++, '   ')
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[24][1]++;
    }

    // Verify password
    const isValidPassword =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[78]++, await bcrypt.compare(password, user.password));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[79]++;
    if (!isValidPassword) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[27][0]++;
      cov_wmf6zey3v().s[80]++;
      return res.status(401).json({
        error: 'Invalid credentials',
        message: `Invalid ${loginType} or password`,
        messageBn: loginType === 'email' ?
        /* istanbul ignore next */
        (cov_wmf6zey3v().b[28][0]++, '   ') :
        /* istanbul ignore next */
        (cov_wmf6zey3v().b[28][1]++, '   ')
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[27][1]++;
    }

    // Check if user is verified
    cov_wmf6zey3v().s[81]++;
    if (user.status === 'PENDING') {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[29][0]++;
      const verificationMessage =
      /* istanbul ignore next */
      (cov_wmf6zey3v().s[82]++, loginType === 'email' ?
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[30][0]++, 'Please verify your email address before logging in') :
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[30][1]++, 'Please verify your phone number before logging in'));
      const verificationMessageBn =
      /* istanbul ignore next */
      (cov_wmf6zey3v().s[83]++, loginType === 'email' ?
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[31][0]++, '     ') :
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[31][1]++, '      '));
      /* istanbul ignore next */
      cov_wmf6zey3v().s[84]++;
      return res.status(403).json({
        error: 'Account not verified',
        message: verificationMessage,
        messageBn: verificationMessageBn,
        requiresVerification: true,
        verificationType: loginType
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[29][1]++;
    }

    // Update last login
    cov_wmf6zey3v().s[85]++;
    await prisma.user.update({
      where: {
        id: user.id
      },
      data: {
        lastLoginAt: new Date()
      }
    });

    // Generate JWT token
    const jwtSecretLogin =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[86]++, process.env.JWT_SECRET);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[87]++;
    if (!jwtSecretLogin) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[32][0]++;
      cov_wmf6zey3v().s[88]++;
      throw new Error('JWT_SECRET environment variable is required');
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[32][1]++;
    }
    const token =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[89]++, jwt.sign({
      userId: user.id,
      email: user.email,
      phone: user.phone,
      role: user.role
    }, jwtSecretLogin, {
      expiresIn: '7d'
    }));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[90]++;
    res.json({
      message: 'Login successful',
      messageBn: ' ',
      user: {
        id: user.id,
        email: user.email,
        phone: user.phone,
        firstName: user.firstName,
        lastName: user.lastName,
        role: user.role,
        status: user.status
      },
      token,
      loginType
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_wmf6zey3v().s[91]++;
    console.error('Login error:', error);
    /* istanbul ignore next */
    cov_wmf6zey3v().s[92]++;
    res.status(500).json({
      error: 'Login failed',
      message: process.env.NODE_ENV === 'development' ?
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[33][0]++, error.message) :
      /* istanbul ignore next */
      (cov_wmf6zey3v().b[33][1]++, 'Internal server error'),
      messageBn: '  '
    });
  }
});

