        verified: false,
        status: 'ERROR',
        message: 'Unable to check verification status'
      };
    }
  }
}

// Singleton instance
const emailVerificationMiddleware =
/* istanbul ignore next */
(cov_3fxiyntgg().s[68]++, new EmailVerificationMiddleware());
/* istanbul ignore next */
cov_3fxiyntgg().s[69]++;
