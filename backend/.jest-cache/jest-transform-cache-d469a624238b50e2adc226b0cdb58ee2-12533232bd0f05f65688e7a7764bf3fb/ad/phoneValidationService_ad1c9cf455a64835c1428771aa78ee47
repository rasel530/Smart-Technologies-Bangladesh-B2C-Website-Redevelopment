  /**
   * Check if phone number is from a specific operator
   * @param {string} phone - Phone number
   * @param {string} operator - Operator name
   * @returns {boolean} True if matches operator
   */
  isOperator(phone, operator) {
    /* istanbul ignore next */
    cov_33nctabpi().f[14]++;
    const operatorInfo =
    /* istanbul ignore next */
    (cov_33nctabpi().s[93]++, this.getOperatorInfo(phone));
    /* istanbul ignore next */
    cov_33nctabpi().s[94]++;
    if (!operatorInfo) {
      /* istanbul ignore next */
      cov_33nctabpi().b[38][0]++;
      cov_33nctabpi().s[95]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_33nctabpi().b[38][1]++;
    }
    cov_33nctabpi().s[96]++;
    return operatorInfo.operator.toLowerCase() === operator.toLowerCase();
  }

