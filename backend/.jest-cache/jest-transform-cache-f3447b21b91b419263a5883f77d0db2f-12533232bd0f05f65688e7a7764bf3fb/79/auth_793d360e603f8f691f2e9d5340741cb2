// Change password endpoint
/* istanbul ignore next */
cov_wmf6zey3v().s[203]++;
router.post('/change-password', [authMiddleware.authenticate(), body('currentPassword').notEmpty().trim(), body('newPassword').isLength({
  min: 8,
  max: 128
}), body('confirmPassword').notEmpty().trim()], handleValidationErrors, async (req, res) => {
  /* istanbul ignore next */
  cov_wmf6zey3v().f[10]++;
  cov_wmf6zey3v().s[204]++;
  try {
    const {
      currentPassword,
      newPassword,
      confirmPassword
    } =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[205]++, req.body);
    const userId =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[206]++, req.user.id);

    // Check if new passwords match
    /* istanbul ignore next */
    cov_wmf6zey3v().s[207]++;
    if (newPassword !== confirmPassword) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[65][0]++;
      cov_wmf6zey3v().s[208]++;
      return res.status(400).json({
        error: 'Passwords do not match',
        message: 'New password and confirmation password must be same',
        messageBn: '     '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[65][1]++;
    }

    // Get user with current password
    const user =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[209]++, await prisma.user.findUnique({
      where: {
        id: userId
      },
      select: {
        id: true,
        email: true,
        firstName: true,
        lastName: true,
        phone: true,
        password: true
      }
    }));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[210]++;
    if (!user) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[66][0]++;
      cov_wmf6zey3v().s[211]++;
      return res.status(404).json({
        error: 'User not found',
        message: 'User account not found',
        messageBn: '  '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[66][1]++;
    }

    // Verify current password
    const isCurrentPasswordValid =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[212]++, await passwordService.verifyPassword(currentPassword, user.password));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[213]++;
    if (!isCurrentPasswordValid) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[67][0]++;
      cov_wmf6zey3v().s[214]++;
      return res.status(401).json({
        error: 'Invalid current password',
        message: 'The current password you entered is incorrect',
        messageBn: '     '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[67][1]++;
    }

    // Check if new password is same as current password
    const isSameAsCurrent =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[215]++, await passwordService.verifyPassword(newPassword, user.password));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[216]++;
    if (isSameAsCurrent) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[68][0]++;
      cov_wmf6zey3v().s[217]++;
      return res.status(400).json({
        error: 'Same password',
        message: 'New password must be different from current password',
        messageBn: '      '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[68][1]++;
    }

    // Validate new password strength
    const userInfo =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[218]++, {
      firstName: user.firstName,
      lastName: user.lastName,
      email: user.email,
      phone: user.phone
    });
    const passwordValidation =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[219]++, passwordService.validatePasswordStrength(newPassword, userInfo));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[220]++;
    if (!passwordValidation.isValid) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[69][0]++;
      cov_wmf6zey3v().s[221]++;
      return res.status(400).json({
        error: 'Password does not meet requirements',
        message: 'New password is too weak or does not meet security requirements',
        messageBn: '     ',
        details: {
          strength: passwordValidation.strength,
          score: passwordValidation.score,
          feedback: passwordValidation.feedback,
          warnings: passwordValidation.warnings,
          suggestions: passwordValidation.suggestions,
          passwordPolicy: passwordService.getPasswordPolicy()
        }
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[69][1]++;
    }

    // Check if password has been used before
    const isPasswordReused =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[222]++, await passwordService.isPasswordReused(userId, newPassword));
    /* istanbul ignore next */
    cov_wmf6zey3v().s[223]++;
    if (isPasswordReused) {
      /* istanbul ignore next */
      cov_wmf6zey3v().b[70][0]++;
      cov_wmf6zey3v().s[224]++;
      return res.status(400).json({
        error: 'Password already used',
        message: 'You cannot reuse a password that has been used in the last 5 changes',
        messageBn: '  5        '
      });
    } else
    /* istanbul ignore next */
    {
      cov_wmf6zey3v().b[70][1]++;
    }

    // Hash new password
    const hashedNewPassword =
    /* istanbul ignore next */
    (cov_wmf6zey3v().s[225]++, await passwordService.hashPassword(newPassword));

    // Update user password
    /* istanbul ignore next */
    cov_wmf6zey3v().s[226]++;
    await prisma.user.update({
      where: {
        id: userId
      },
