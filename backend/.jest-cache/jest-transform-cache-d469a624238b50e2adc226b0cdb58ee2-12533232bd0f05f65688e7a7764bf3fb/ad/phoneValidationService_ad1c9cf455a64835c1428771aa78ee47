  /**
   * Normalize phone number to international format
   * @param {string} phone - Phone number to normalize
   * @returns {string|null} Normalized phone number or null if invalid
   */
  normalizePhoneNumber(phone) {
    /* istanbul ignore next */
    cov_33nctabpi().f[11]++;
    const validation =
    /* istanbul ignore next */
    (cov_33nctabpi().s[66]++, this.validateBangladeshPhoneNumber(phone));
    /* istanbul ignore next */
    cov_33nctabpi().s[67]++;
    if (!validation.isValid) {
      /* istanbul ignore next */
      cov_33nctabpi().b[27][0]++;
      cov_33nctabpi().s[68]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_33nctabpi().b[27][1]++;
    }
    cov_33nctabpi().s[69]++;
    return validation.normalizedPhone;
  }

  /**
   * Format phone number for display
   * @param {string} phone - Phone number to format
   * @param {string} format - Desired format ('international', 'local', 'display')
   * @returns {string} Formatted phone number
   */
  formatPhoneNumber(phone, format =
  /* istanbul ignore next */
  (cov_33nctabpi().b[28][0]++, 'display')) {
    /* istanbul ignore next */
    cov_33nctabpi().f[12]++;
    const validation =
    /* istanbul ignore next */
    (cov_33nctabpi().s[70]++, this.validateBangladeshPhoneNumber(phone));
    /* istanbul ignore next */
    cov_33nctabpi().s[71]++;
    if (!validation.isValid) {
      /* istanbul ignore next */
      cov_33nctabpi().b[29][0]++;
      cov_33nctabpi().s[72]++;
      return phone;
    } else
    /* istanbul ignore next */
    {
      cov_33nctabpi().b[29][1]++;
    }
    const normalized =
    /* istanbul ignore next */
    (cov_33nctabpi().s[73]++, validation.normalizedPhone);
    /* istanbul ignore next */
    cov_33nctabpi().s[74]++;
    switch (format) {
      case 'international':
        /* istanbul ignore next */
        cov_33nctabpi().b[30][0]++;
        cov_33nctabpi().s[75]++;
        return normalized;
      case 'local':
        /* istanbul ignore next */
        cov_33nctabpi().b[30][1]++;
        cov_33nctabpi().s[76]++;
        if (normalized.startsWith('+880')) {
          /* istanbul ignore next */
          cov_33nctabpi().b[31][0]++;
          cov_33nctabpi().s[77]++;
          return normalized.substring(3);
        } else
        /* istanbul ignore next */
        {
          cov_33nctabpi().b[31][1]++;
        }
        cov_33nctabpi().s[78]++;
        return normalized;
      case 'display':
        /* istanbul ignore next */
        cov_33nctabpi().b[30][2]++;
        cov_33nctabpi().s[79]++;
        if (validation.type === 'mobile') {
          /* istanbul ignore next */
          cov_33nctabpi().b[32][0]++;
          cov_33nctabpi().s[80]++;
          if (normalized.startsWith('+880')) {
            /* istanbul ignore next */
            cov_33nctabpi().b[33][0]++;
            const rest =
            /* istanbul ignore next */
            (cov_33nctabpi().s[81]++, normalized.substring(4));
            /* istanbul ignore next */
            cov_33nctabpi().s[82]++;
            return `+880 ${rest.slice(0, 3)} ${rest.slice(3, 6)} ${rest.slice(6, 10)}`;
          } else
          /* istanbul ignore next */
          {
            cov_33nctabpi().b[33][1]++;
          }
        } else {
          /* istanbul ignore next */
          cov_33nctabpi().b[32][1]++;
          cov_33nctabpi().s[83]++;
          if (validation.type === 'landline') {
            /* istanbul ignore next */
            cov_33nctabpi().b[34][0]++;
            cov_33nctabpi().s[84]++;
            if (normalized.startsWith('+880')) {
              /* istanbul ignore next */
              cov_33nctabpi().b[35][0]++;
              const rest =
              /* istanbul ignore next */
              (cov_33nctabpi().s[85]++, normalized.substring(4));
              /* istanbul ignore next */
              cov_33nctabpi().s[86]++;
              return `+880 ${rest.slice(0, 2)} ${rest.slice(2, 6)} ${rest.slice(6, 10)}`;
            } else
            /* istanbul ignore next */
            {
              cov_33nctabpi().b[35][1]++;
            }
          } else
          /* istanbul ignore next */
          {
            cov_33nctabpi().b[34][1]++;
          }
        }
        /* istanbul ignore next */
        cov_33nctabpi().s[87]++;
        return normalized;
      default:
        /* istanbul ignore next */
        cov_33nctabpi().b[30][3]++;
        cov_33nctabpi().s[88]++;
        return normalized;
    }
  }

  /**
   * Get operator information
   * @param {string} phone - Phone number
   * @returns {Object|null} Operator information
   */
  getOperatorInfo(phone) {
    /* istanbul ignore next */
    cov_33nctabpi().f[13]++;
    const validation =
    /* istanbul ignore next */
    (cov_33nctabpi().s[89]++, this.validateBangladeshPhoneNumber(phone));
    /* istanbul ignore next */
    cov_33nctabpi().s[90]++;
    if (
    /* istanbul ignore next */
    (cov_33nctabpi().b[37][0]++, !validation.isValid) ||
    /* istanbul ignore next */
    (cov_33nctabpi().b[37][1]++, validation.type !== 'mobile')) {
      /* istanbul ignore next */
      cov_33nctabpi().b[36][0]++;
