    // Mobile number patterns
    const mobilePatterns =
    /* istanbul ignore next */
    (cov_33nctabpi().s[24]++, [/^\+8801[3-9]\d{8}$/,
    // International format: +8801XXXXXXXXX
    /^8801[3-9]\d{8}$/,
    // Without +: 8801XXXXXXXXX
    /^01[3-9]\d{8}$/ // Local format: 01XXXXXXXXX
    ]);
    /* istanbul ignore next */
    cov_33nctabpi().s[25]++;
    if (!mobilePatterns.some(pattern => {
      /* istanbul ignore next */
      cov_33nctabpi().f[3]++;
      cov_33nctabpi().s[26]++;
      return pattern.test(phone);
    })) {
      /* istanbul ignore next */
      cov_33nctabpi().b[14][0]++;
      cov_33nctabpi().s[27]++;
      return {
        isValid: false,
        error: 'Invalid mobile number format',
        errorBn: '   ',
        code: 'INVALID_MOBILE_FORMAT'
      };
    } else
    /* istanbul ignore next */
    {
      cov_33nctabpi().b[14][1]++;
    }

    // Normalize to international format
    let normalizedPhone =
    /* istanbul ignore next */
    (cov_33nctabpi().s[28]++, phone);
    /* istanbul ignore next */
    cov_33nctabpi().s[29]++;
    if (phone.startsWith('01')) {
      /* istanbul ignore next */
      cov_33nctabpi().b[15][0]++;
      cov_33nctabpi().s[30]++;
      normalizedPhone = `+880${phone}`;
    } else {
      /* istanbul ignore next */
      cov_33nctabpi().b[15][1]++;
      cov_33nctabpi().s[31]++;
      if (phone.startsWith('880')) {
        /* istanbul ignore next */
        cov_33nctabpi().b[16][0]++;
        cov_33nctabpi().s[32]++;
        normalizedPhone = `+${phone}`;
      } else
      /* istanbul ignore next */
      {
        cov_33nctabpi().b[16][1]++;
      }
    }

    // Extract operator prefix
    const prefix =
    /* istanbul ignore next */
    (cov_33nctabpi().s[33]++, normalizedPhone.substring(3, 6));
    const operator =
    /* istanbul ignore next */
    (cov_33nctabpi().s[34]++, this.mobileOperators[prefix]);
    /* istanbul ignore next */
    cov_33nctabpi().s[35]++;
    if (!operator) {
      /* istanbul ignore next */
      cov_33nctabpi().b[17][0]++;
      cov_33nctabpi().s[36]++;
      return {
        isValid: false,
        error: 'Unsupported mobile operator',
        errorBn: '  ',
        code: 'UNSUPPORTED_OPERATOR',
        supportedOperators: Object.keys(this.mobileOperators).map(p => {
          /* istanbul ignore next */
          cov_33nctabpi().f[4]++;
          cov_33nctabpi().s[37]++;
          return this.mobileOperators[p].name;
        })
      };
    } else
    /* istanbul ignore next */
    {
      cov_33nctabpi().b[17][1]++;
    }
    cov_33nctabpi().s[38]++;
    return {
      isValid: true,
      type: 'mobile',
      format: this.detectFormat(phone),
      normalizedPhone,
      operator: operator.name,
      operatorDetails: {
        prefix,
        ...operator
      },
      network: operator.network,
      isPorted: false,
      // TODO: Implement portability check if needed
      metadata: {
        length: normalizedPhone.length,
        countryCode: '+880',
        numberWithoutCountry: normalizedPhone.substring(3)
      }
    };
  }

  /**
   * Validate landline numbers
   * @param {string} phone - Clean phone number
   * @returns {Object} Landline validation result
   */
  validateLandlineNumber(phone) {
    /* istanbul ignore next */
    cov_33nctabpi().f[5]++;
    // Landline patterns
    const landlinePatterns =
    /* istanbul ignore next */
    (cov_33nctabpi().s[39]++, [/^\+8802[1-9]\d{7}$/,
    // International: +8802XXXXXXXX
    /^8802[1-9]\d{7}$/,
    // Without +: 8802XXXXXXXX
    /^02[1-9]\d{7}$/ // Local: 02XXXXXXXX
    ]);
    /* istanbul ignore next */
    cov_33nctabpi().s[40]++;
    if (!landlinePatterns.some(pattern => {
      /* istanbul ignore next */
      cov_33nctabpi().f[6]++;
      cov_33nctabpi().s[41]++;
      return pattern.test(phone);
    })) {
      /* istanbul ignore next */
      cov_33nctabpi().b[18][0]++;
      cov_33nctabpi().s[42]++;
      return {
        isValid: false,
        error: 'Invalid landline number format',
        errorBn: '   ',
        code: 'INVALID_LANDLINE_FORMAT'
      };
    } else
    /* istanbul ignore next */
    {
      cov_33nctabpi().b[18][1]++;
    }

    // Normalize to international format
    let normalizedPhone =
    /* istanbul ignore next */
    (cov_33nctabpi().s[43]++, phone);
    /* istanbul ignore next */
    cov_33nctabpi().s[44]++;
    if (phone.startsWith('02')) {
      /* istanbul ignore next */
      cov_33nctabpi().b[19][0]++;
      cov_33nctabpi().s[45]++;
      normalizedPhone = `+880${phone}`;
    } else {
      /* istanbul ignore next */
      cov_33nctabpi().b[19][1]++;
      cov_33nctabpi().s[46]++;
      if (phone.startsWith('8802')) {
        /* istanbul ignore next */
        cov_33nctabpi().b[20][0]++;
        cov_33nctabpi().s[47]++;
        normalizedPhone = `+${phone}`;
      } else
      /* istanbul ignore next */
      {
        cov_33nctabpi().b[20][1]++;
      }
    }

    // Extract area code
    let areaCode;
    /* istanbul ignore next */
    cov_33nctabpi().s[48]++;
    if (normalizedPhone.startsWith('+8802')) {
      /* istanbul ignore next */
      cov_33nctabpi().b[21][0]++;
      cov_33nctabpi().s[49]++;
      areaCode = normalizedPhone.substring(3, 5);
    } else
    /* istanbul ignore next */
    {
      cov_33nctabpi().b[21][1]++;
    }
    const areaInfo =
    /* istanbul ignore next */
    (cov_33nctabpi().s[50]++, this.landlineAreaCodes[areaCode]);
    /* istanbul ignore next */
    cov_33nctabpi().s[51]++;
    return {
      isValid: true,
      type: 'landline',
      format: this.detectFormat(phone),
      normalizedPhone,
      areaCode,
      areaInfo:
      /* istanbul ignore next */
      (cov_33nctabpi().b[22][0]++, areaInfo) ||
      /* istanbul ignore next */
      (cov_33nctabpi().b[22][1]++, {
        area: 'Unknown',
        region: 'Unknown'
      }),
      metadata: {
        length: normalizedPhone.length,
        countryCode: '+880',
        numberWithoutCountry: normalizedPhone.substring(3)
      }
    };
  }

  /**
   * Validate special numbers (emergency, toll-free, etc.)
   * @param {string} phone - Clean phone number
   * @returns {Object} Special number validation result
   */
  validateSpecialNumbers(phone) {
    /* istanbul ignore next */
    cov_33nctabpi().f[7]++;
    cov_33nctabpi().s[52]++;
    for (const [type, info] of Object.entries(this.specialNumbers)) {
      /* istanbul ignore next */
      cov_33nctabpi().s[53]++;
      for (const pattern of info.patterns) {
        /* istanbul ignore next */
        cov_33nctabpi().s[54]++;
        if (pattern.test(phone)) {
          /* istanbul ignore next */
          cov_33nctabpi().b[23][0]++;
          cov_33nctabpi().s[55]++;
          return {
